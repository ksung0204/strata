{
    "version": 2,
    "builds": [
      {
        "src": "next.config.ts",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.ts"
      }
    ],
    "regions": ["syd1"],
    "buildCommand": "npm run build",
    "crons": [
      {
        "path": "/api/refresh",
        "schedule": "0 0 * * *"
      }
    ]
  }
  