{
    "version": 2,
    "builds": [
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      }
    ],
    "regions": ["syd1"],
    "buildCommand": "npm run build",
    "crons": [
      {
        "path": "/api/refresh",
        "schedule": "0 0 * * *"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
  }

